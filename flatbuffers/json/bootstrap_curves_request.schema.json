        "tenor_number" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Used only when forward_type=Period, compute forward over [d, d + tenor]."
              },
        "tenor_time_unit" : {
                "$ref" : "#/definitions/quantra_enums_TimeUnit"
              },
        "use_grid_calendar_for_advance" : {
                "type" : "boolean",
                "description" : "If true, use calendar from grid for date advancement.\nIf false, use curve's instrument calendar."
