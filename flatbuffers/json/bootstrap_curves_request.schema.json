    "quantra_Tenor" : {
      "type" : "object",
      "description" : "A tenor as a structured {n, unit} pair that maps directly to QuantLib::Period.\nExamples: {n:1, unit:Days}, {n:3, unit:Months}, {n:10, unit:Years}",
      "properties" : {
        "n" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647
              },
        "unit" : {
                "$ref" : "#/definitions/quantra_enums_TimeUnit"
              }
      },
      "additionalProperties" : false
    },
    "quantra_TenorGrid" : {
      "type" : "object",
      "description" : "Tenors relative to the curve reference date (or as_of_date if no reference date).\nExamples: [{n:1,unit:Days},{n:1,unit:Weeks},{n:3,unit:Months},{n:10,unit:Years}]\n \nCalendar behavior:\n- If calendar is NullCalendar (default): raw date arithmetic (refDate + period)\n- If calendar is set: uses calendar.advance(refDate, period, bdc)",
      "properties" : {
        "tenors" : {
                "type" : "array", "items" : {"$ref" : "#/definitions/quantra_Tenor"}
              },
        "calendar" : {
                "$ref" : "#/definitions/quantra_enums_Calendar",
                "description" : "Calendar for business-day-adjusted tenor dates.\nDefault NullCalendar means raw date arithmetic without business day adjustment."
              },
        "business_day_convention" : {
                "$ref" : "#/definitions/quantra_enums_BusinessDayConvention",
                "description" : "Business day convention for advancing dates (used when calendar is not NullCalendar)."
              }
      },
      "required" : ["tenors"],
      "additionalProperties" : false
    },
    "quantra_RangeGrid" : {
      "type" : "object",
      "description" : "Sample the curve between start/end with a step.\nUse this for \"daily curve until X\".\n\nNote on business_days_only:\n- Requires a real calendar (not NullCalendar) to skip weekends/holidays\n- If business_days_only=true with NullCalendar, falls back to WeekendsOnly",
      "properties" : {
        "start_date" : {
                "type" : "string",
                "description" : "If omitted, server defaults to as_of_date."
              },
        "end_date" : {
                "type" : "string",
                "description" : "Required end date (YYYY-MM-DD or YYYY/MM/DD)."
              },
        "step_number" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Step size, e.g. 1 Days for daily, 1 Weeks, 1 Months."
              },
        "step_time_unit" : {
                "$ref" : "#/definitions/quantra_enums_TimeUnit"
              },
        "business_days_only" : {
                "type" : "boolean",
                "description" : "If true, generate only business days using the calendar below.\nRequires a non-NullCalendar to properly skip weekends/holidays."
              },
        "calendar" : {
                "$ref" : "#/definitions/quantra_enums_Calendar",
                "description" : "Calendar used for business day checks and date advancement."
              },
        "business_day_convention" : {
                "$ref" : "#/definitions/quantra_enums_BusinessDayConvention",
                "description" : "Business day convention used when adjusting generated dates."
              }
      },
      "required" : ["end_date"],
      "additionalProperties" : false
    },
