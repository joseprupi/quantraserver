      "required" : ["date"],
      "additionalProperties" : false
    },
    "quantra_IndexDef" : {
      "type" : "object",
      "description" : "Complete index definition. Registered once per request, referenced by id.",
      "properties" : {
        "id" : {
                "type" : "string",
                "description" : "Unique identifier (e.g., \"EUR_6M\", \"USD_SOFR\", \"MY_CUSTOM_3M\")"
              },
        "name" : {
                "type" : "string",
                "description" : "Human-readable name / family label (e.g., \"Euribor\", \"SOFR\", \"MyBank3M\")\nUsed as the QuantLib index familyName. No financial logic depends on this."
              },
        "index_type" : {
                "$ref" : "#/definitions/quantra_IndexType",
                "description" : "IBOR or Overnight. Determines QuantLib class used."
              },
        "tenor_number" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Tenor period (e.g., 6 Months for Euribor 6M, 0 for overnight)\nFor overnight indices, set tenor_number=0."
              },
        "tenor_time_unit" : {
                "$ref" : "#/definitions/quantra_enums_TimeUnit"
              },
        "fixing_days" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Fixing days (e.g., 2 for Euribor, 0 for SOFR/ESTR)"
              },
        "calendar" : {
                "$ref" : "#/definitions/quantra_enums_Calendar",
                "description" : "Calendar for fixing/payment dates"
              },
        "business_day_convention" : {
                "$ref" : "#/definitions/quantra_enums_BusinessDayConvention",
                "description" : "Business day convention"
              },
        "day_counter" : {
                "$ref" : "#/definitions/quantra_enums_DayCounter",
                "description" : "Day count convention"
              },
        "end_of_month" : {
                "type" : "boolean",
                "description" : "End of month rule (IBOR only, ignored for overnight)"
              },
        "currency" : {
                "type" : "string",
                "description" : "ISO currency code (e.g., \"EUR\", \"USD\", \"GBP\", \"JPY\")\nMapped to QuantLib Currency objects. Required."
              },
        "fixings" : {
                "type" : "array", "items" : {"$ref" : "#/definitions/quantra_Fixing"},
                "description" : "Past fixings needed for seasoned instruments.\nApplied via index->addFixing() before pricing."
              }
      },
      "required" : ["id", "name"],
      "additionalProperties" : false
    },
    "quantra_IndexRef" : {
      "type" : "object",
      "description" : "Reference to an IndexDef by its id.\nUsed by helpers and instruments to point to a registered index.",
      "properties" : {
        "id" : {
                "type" : "string"
              }
      },
      "required" : ["id"],
      "additionalProperties" : false
    }
